<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.wechatui" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_fanxin"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".SecondActivity"/>

        <activity android:name=".ThirdActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/horizontal_slide"
            android:windowSoftInputMode="adjustPan"/>

        <activity android:name=".FourthActivity"/>
        <activity android:name=".FiveActivity"/>
        <activity android:name=".SexActivity"/>
        <activity android:name=".SevenActivity"/>
        <activity android:name=".EightActivity"/>

    </application>
</manifest>